#include<iostream>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--){
	    int a,b,c;
	    cin>>a>>b>>c;
	    if((a>b and a<c) or (a>c and a<b)) cout<<a<<endl; // for checking a
	    else if((b>a and b<c) or (b>c and b<a)) cout<<b<<endl; // for checking b
	    else cout<<c<<endl; // for checking c.
	}
	return 0;
}
