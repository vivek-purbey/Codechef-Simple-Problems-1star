#include<iostream>
using namespace std;

int main(){
	int t;
	cin>>t;
	while(t--){
	    int n,k,m;
	    cin>>n>>k;
	    int res=k;
	    for(int i=1;i<=n;i++)
	    {
	        cin>>m;
	        if(res>=m){
	        cout<<1;
	        res=res-m;}
	        else cout<<0;
	    } cout<<endl;
	}
	return 0;
}
