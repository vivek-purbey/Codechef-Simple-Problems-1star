#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
	    int n,b,res,m=-999,q=0;
	    cin>>n>>b;
	    for(int i=1;i<=n;i++)
	    {
	        int w,h,p;
	        cin>>w>>h>>p;
	        if(b>=p){
	            res=w*h;
	            m=max(res, m);
	        }
	    }
	    if(m!=-999) cout<<m<<endl;
	    if(m==-999) cout<<"no tablet"<<endl; //if(m==-999)/else.
	}
	return 0;
}
